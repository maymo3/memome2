block variable

doctype html
html(lang="ja")
    head
        meta(charset="utf-8")
        meta(name="viewport" content="width=device-width, initial-scale=1.0")
        title memome
        link(rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Noto+Sans+JP:wght@500&display=swap")
        link(rel="stylesheet" href= css_path + "/css/normalize.css")
        link(rel="stylesheet" href= css_path + "/css/solarized-light.css")
        link(rel="stylesheet" href="https://use.fontawesome.com/releases/v5.12.1/css/all.css")
        link(rel="stylesheet" href= css_path + "/css/style.css" type="text/css")

        script(src= css_path + "/js/highlight.pack.js")
        script(src= css_path + "/js/jquery-3.4.1.min.js")
        script(src= css_path + "/js/script.js")
    body
        if category_name != ''
            .left-back
            .container
                .contents
                    aside#side.side
                        #toggle-btn.toggle-btn
                            span
                            span
                            span
                        h2.pao #[a(href="../index.html") memome]#[i.fas.fa-chevron-right] #{category_name}
                        ul.list-unstyled
                            each article in category_articles
                                li: a.text-dark.font-weight-light(href= article.path + '.html') #{article.title}
                    main.main
                        block content
        else
            block content
        script hljs.initHighlightingOnLoad();